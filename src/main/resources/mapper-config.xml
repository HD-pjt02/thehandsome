<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="REUSE" />
		<setting name="defaultStatementTimeout" value="60" />
		<setting name="callSettersOnNulls" value="true" />
		<setting name="mapUnderscoreToCamelCase" value="false" />
	</settings>

	<typeAliases>
		<typeAlias alias="QnaDTO"
			type="com.thehandsome.app.dto.QnaDTO" />
		<typeAlias alias="OrderDTO"
			type="com.thehandsome.app.dto.OrderDTO" />
		<typeAlias alias="ProductDTO"
			type="com.thehandsome.app.dto.ProductDTO" />
		<typeAlias alias="CategoryDTO"
			type="com.thehandsome.app.dto.CategoryDTO" />
		<typeAlias alias="BrandDTO"
			type="com.thehandsome.app.dto.BrandDTO" />
		<typeAlias alias="ColorDTO"
			type="com.thehandsome.app.dto.ColorDTO" />
		<typeAlias alias="StockDTO"
			type="com.thehandsome.app.dto.StockDTO" />
		<typeAlias alias="MemberDTO"
			type="com.thehandsome.app.dto.MemberDTO" />
		<typeAlias alias="CartDTO"
			type="com.thehandsome.app.dto.CartDTO" />
		<typeAlias alias="ReviewDTO"
			type="com.thehandsome.app.dto.ReviewDTO" />
	</typeAliases>
</configuration>