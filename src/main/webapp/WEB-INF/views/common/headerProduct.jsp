	}else{
		lookBookbxslider.css('transition-duration','0.5s');
		lookBookbxslider.css('transform','translate3d('+prevMoveImageWidth+'px,0px,0px)');
	}
	$(".bx-prev").show();
	$(".lookbook_prev").hide();
	moveYn=true;
}



